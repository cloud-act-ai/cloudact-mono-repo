# =============================================
# CLOUDACT PIPELINE SERVICE - Dev-Cloud Environment
# =============================================
# Local development pointing to cloudact-testing-1 GCP project
# Must share KMS + Root API Key with API service
# =============================================

# =============================================
# GCP CONFIGURATION
# =============================================
GCP_PROJECT_ID=cloudact-testing-1
GOOGLE_APPLICATION_CREDENTIALS=/home/user/.gcp/cloudact-testing-1-sa.json
BIGQUERY_LOCATION=US

# =============================================
# KMS ENCRYPTION (must match api-service)
# =============================================
KMS_PROJECT_ID=cloudact-testing-1
KMS_LOCATION=us-central1
KMS_KEYRING=cloudact-keyring-test
KMS_KEY=api-key-encryption

# =============================================
# SECURITY (must match api-service)
# =============================================
CA_ROOT_API_KEY=test-api-key-for-cloudact-testing-32chars
API_KEY_SECRET_KEY=test-api-key-secret-for-signing-32ch
DISABLE_AUTH=false
RATE_LIMIT_ENABLED=true

# =============================================
# APPLICATION
# =============================================
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG
API_HOST=0.0.0.0
API_PORT=8001
API_WORKERS=1
API_RELOAD=true
ENABLE_API_DOCS=true

# =============================================
# PIPELINE EXECUTION
# =============================================
PIPELINE_MAX_PARALLEL_STEPS=10
PIPELINE_GLOBAL_CONCURRENT_LIMIT=100
FALLBACK_DAILY_LIMIT=6
FALLBACK_MONTHLY_LIMIT=180
FALLBACK_CONCURRENT_LIMIT=6

# =============================================
# API SERVICE URL
# =============================================
API_SERVICE_URL=http://localhost:8000

# =============================================
# SUPABASE (Test Instance - quota tracking)
# =============================================
SUPABASE_URL=https://kwroaccbrxppfiysqlzs.supabase.co
SUPABASE_SERVICE_ROLE_KEY=REPLACE_WITH_SUPABASE_SERVICE_ROLE_KEY

# =============================================
# STORED PROCEDURES AUTO-SYNC
# =============================================
AUTO_SYNC_PROCEDURES=true

# =============================================
# NOTIFICATIONS (Disabled for dev)
# =============================================
NOTIFICATIONS_ENABLED=false
