{
  "schema": [
    {"name": "subscription_id", "type": "STRING", "mode": "REQUIRED", "description": "Unique subscription identifier"},
    {"name": "plan_name", "type": "STRING", "mode": "REQUIRED", "description": "Plan name (FREE, PAY_AS_YOU_GO, etc.)"},
    {"name": "quantity", "type": "INTEGER", "mode": "REQUIRED", "description": "Number of subscriptions (default: 0)"},
    {"name": "unit_price_usd", "type": "FLOAT64", "mode": "REQUIRED", "description": "Price per unit in USD"},
    {"name": "effective_date", "type": "DATE", "mode": "REQUIRED", "description": "Date when this subscription became effective"},
    {"name": "notes", "type": "STRING", "mode": "NULLABLE", "description": "Additional notes"},
    {"name": "created_at", "type": "TIMESTAMP", "mode": "REQUIRED", "description": "Record creation timestamp"},
    {"name": "updated_at", "type": "TIMESTAMP", "mode": "REQUIRED", "description": "Last update timestamp"}
  ],
  "clustering_fields": ["plan_name"]
}
