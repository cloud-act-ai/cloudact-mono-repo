{
  "schema": [
    {"name": "model_id", "type": "STRING", "mode": "REQUIRED", "description": "Unique model identifier (e.g., deepseek-chat)"},
    {"name": "model_name", "type": "STRING", "mode": "NULLABLE", "description": "Human-readable model name"},
    {"name": "input_price_per_1k", "type": "FLOAT64", "mode": "REQUIRED", "description": "Price per 1K input tokens in USD"},
    {"name": "output_price_per_1k", "type": "FLOAT64", "mode": "REQUIRED", "description": "Price per 1K output tokens in USD"},
    {"name": "effective_date", "type": "DATE", "mode": "REQUIRED", "description": "Date when this pricing became effective"},
    {"name": "notes", "type": "STRING", "mode": "NULLABLE", "description": "Additional notes about this pricing"},
    {"name": "created_at", "type": "TIMESTAMP", "mode": "REQUIRED", "description": "Record creation timestamp"},
    {"name": "updated_at", "type": "TIMESTAMP", "mode": "REQUIRED", "description": "Last update timestamp"}
  ],
  "clustering_fields": ["model_id"]
}
